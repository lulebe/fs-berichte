{% extends "app/admin/adminreportbase.twig" %}

{% block admincontent %}
  <h1>Prüfungsart anlegen</h1>
  <form method="POST">
    <div class="inputlabel">
      <label for="examtypenameinput">Prüfungsname</label>
      <input type="text" required maxlength="50" name="examtypename" id="examtypenameinput" placeholder="Name">
    </div>
    <br>
    <small>Fächeranzahl</small>
    <div class="radiorow">
      <div class="grouper">
        <input type="radio" class="lbl" required name="examtypesubjectcount" id="examtypesubjectcountinput1" value="1">
        <label class="check" for="examtypesubjectcountinput1">1</label>
      </div>
      <div class="grouper">
        <input type="radio" class="lbl" required name="examtypesubjectcount" id="examtypesubjectcountinput2" value="2">
        <label class="check" for="examtypesubjectcountinput2">2</label>
      </div>
      <div class="grouper">
        <input type="radio" class="lbl" required name="examtypesubjectcount" id="examtypesubjectcountinput3" value="3" checked>
        <label class="check" for="examtypesubjectcountinput3">3</label>
      </div>
      <div class="grouper">
        <input type="radio" class="lbl" required name="examtypesubjectcount" id="examtypesubjectcountinput4" value="4">
        <label class="check" for="examtypesubjectcountinput4">4</label>
      </div>
      <div class="grouper">
        <input type="radio" class="lbl" required name="examtypesubjectcount" id="examtypesubjectcountinput5" value="5">
        <label class="check" for="examtypesubjectcountinput5">5</label>
      </div>
      <div class="grouper">
        <input type="radio" class="lbl" required name="examtypesubjectcount" id="examtypesubjectcountinput6" value="6">
        <label class="check" for="examtypesubjectcountinput6">6</label>
      </div>
      <div class="grouper">
        <input type="radio" class="lbl" required name="examtypesubjectcount" id="examtypesubjectcountinput7" value="7">
        <label class="check" for="examtypesubjectcountinput7">7</label>
      </div>
      <div class="grouper">
        <input type="radio" class="lbl" required name="examtypesubjectcount" id="examtypesubjectcountinput8" value="8">
        <label class="check" for="examtypesubjectcountinput8">8</label>
      </div>
      <div class="grouper">
        <input type="radio" class="lbl" required name="examtypesubjectcount" id="examtypesubjectcountinput9" value="9">
        <label class="check" for="examtypesubjectcountinput9">9</label>
      </div>
      <div class="grouper">
        <input type="radio" class="lbl" required name="examtypesubjectcount" id="examtypesubjectcountinput10" value="10">
        <label class="check" for="examtypesubjectcountinput10">10</label>
      </div>
    </div>
    <br><br>
    <input type="submit" value="anlegen">
  </form>
{% endblock %}

